<div id="login" class="span6 offset3" >
  <%= form_tag sessions_path, :method => :post do %>
  <fieldset>
    <%= label_tag :email %>
    <%= email_field_tag :email, params[:email] %>

    <%= label_tag :password %>
    <%= password_field_tag :password %>

    <%= submit_tag "Login", :class => "btn btn-large btn-primary" %>

		
  </fieldset>
<% end %>





